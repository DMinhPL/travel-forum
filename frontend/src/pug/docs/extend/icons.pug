extends ../../layouts/_layout_doc 
block setting
    -var mainActive = 'docs';
    -var slideActive = 'icons';
    - title = "Layout - Icons"
block navsection

block content 
    h1.bd-title#content  Icons
    p.bd-lead Guidance and suggestions for using external icon libraries with Bootstrap.

    h2#responsive Elegant Icon Font

    .bd-example-row
        .bd-example
            +ic('icon_search')
            | &nbsp;
            +ic-primary('icon_search')
            | &nbsp;
            +ic-secondary('icon_search')
            | &nbsp;
            +ic-success('icon_search')
            | &nbsp;
            +ic-info('icon_search')
            | &nbsp;
            +ic-warning('icon_search')
            | &nbsp;
            +ic-danger('icon_search')
            | &nbsp;
            +ic-light('icon_search')
            | &nbsp;
            +ic-dark('icon_search')

        figure.highlight
            pre
                code.js.
                    +ic('icon_search')
                    +ic-primary('icon_search')
                    +ic-secondary('icon_search')
                    +ic-success('icon_search')
                    +ic-info('icon_search')
                    +ic-warning('icon_search')
                    +ic-danger('icon_search')
                    +ic-light('icon_search')
                    +ic-dark('icon_search')
